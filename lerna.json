{
    "$schema": "node_modules/lerna/schemas/lerna-schema.json",
    "version": "1.0.0-beta.0",
    "npmClient": "yarn",
    "packages": [
        "packages/*"
    ],
    "command": {
        "publish": {
            "ignoreChanges": [
                "**/*.test.ts",
                "**/*.spec.ts",
                "**/test/**",
                "**/*.md"
            ]
        }
    }
}
